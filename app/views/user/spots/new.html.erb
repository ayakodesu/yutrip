<h1>User::Spots#new</h1>
<p>Find me in app/views/user/spots/new.html.erb</p>

<h1>新規投稿</h1>


<%= form_with model: @spot, url: user_spots_path do |f| %>
  <h4>温泉名</h4>
  <%= f.text_field :name %>
  <h4>カテゴリー</h4>
  <%#= f.text_field :category_id %>
  <h4>温泉質</h4>
  <%= f.text_field :quality %>

  <h4>本文</h4>
  <%= f.text_area :introduction %>

  <%= f.submit '投稿' %>
<% end %>