<h1>新規投稿</h1>


<%= form_with model: @spot, url: admin_spots_path do |f| %>
  <h4>温泉名</h4>
  <%= f.text_field :name %>
  <h4>カテゴリー</h4>
  <%#= f.text_field :category_id %>
  <h4>温泉質</h4>
  <%= f.text_field :quality %>

  <h4>本文</h4>
  <%= f.text_area :introduction %>

  <%= f.submit '投稿' %>
<% end %>
